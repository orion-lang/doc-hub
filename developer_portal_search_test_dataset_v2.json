  "evaluation_metrics": {
    "precision_at_k": {
      "description": "Percentage of returned results that are relevant",
      "k_values": [3, 5, 10],
      "target_precision": {
        "k3": 0.80,
        "k5": 0.70,
        "k10": 0.60
      }
    },
    "recall_at_k": {
      "description": "Percentage of relevant documents that are returned",
      "k_values": [3, 5, 10],
      "target_recall": {
        "k3": 0.50,
        "k5": 0.70,
        "k10": 0.90
      }
    },
    "mrr": {
      "description": "Mean Reciprocal Rank - position of first relevant result",
      "target": 0.75
    },
    "ndcg": {
      "description": "Normalized Discounted Cumulative Gain",
      "target": 0.80
    }
  },

  "test_execution_guidelines": {
    "batch_testing": "Run all queries in category batches for systematic evaluation",
    "result_logging": "Log query, returned chunks (ranked), timestamps, and latency",
    "content_matching": "Verify returned chunks contain expected content keywords",
    "failure_analysis": "For queries with precision <0.5, analyze failure mode",
    "regression_testing": "Re-run after any index or algorithm changes",
    "a_b_testing": "Compare synonym/typo queries against base queries"
  }
}
